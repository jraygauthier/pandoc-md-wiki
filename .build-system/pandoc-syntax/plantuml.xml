    <contexts>
      <context name="Normal" attribute="NormalText" lineEndContext="#stay">
        <DetectChar attribute="Comment" char="'" firstNonSpace="true" context="SingleQuoteComment"/>
        <DetectChar attribute="ActivityLabel" char=":" firstNonSpace="true" context="ActivityLabel"/>
        <keyword attribute="Type" String="type" firstNonSpace="true" context="TypeDeclaration"/>
        <keyword attribute="Type" String="type2" firstNonSpace="true" context="TypeDeclaration"/>
        <keyword attribute="Keyword" String="keyword" context="#stay"/>
        <keyword attribute="Keyword" String="keyword2" context="#stay"/>
        <keyword attribute="Flow" String="flow" context="#stay"/>
        <keyword attribute="Flow" String="flow2" context="#stay"/>
        <keyword attribute="Preprocessor" String="preprocessor" context="#stay"/>
        <keyword attribute="Skinparameter" String="skinparameter" context="#stay"/>
        <keyword attribute="Color" String="color" context="#stay"/>
        <RegExpr attribute="Link" String="(?:&lt;\||\(\)|[&lt;o\*x\#\+\^\}])([\-\.])" context="FindLinkTailOrOtherHead" dynamic="true"/>
        <RegExpr attribute="Link" String="([\-\.])" context="FindLinkHead" dynamic="true"/>
        <!-- <RegExpr attribute="Relation" String="^[A-Za-z0-9]+[ \t]*[]" context="#stay"/> -->
        <RangeDetect attribute="String" char="&quot;" char1="&quot;" context="#stay"/>
        <DetectChar attribute="NormalText" char=":" context="LinkLabel"/>

      </context>
			<context name="SingleQuoteComment" attribute="Comment" lineEndContext="#pop">
			</context>

			<context name="ActivityLabel" attribute="ActivityLabel" lineEndContext="#stay">
        <DetectChar attribute="ActivityLabel" char=";" context="#pop"/>
			</context>

			<context name="LinkLabel" attribute="LinkLabel" lineEndContext="#pop">
        <DetectSpaces attribute="NormalText" context="#stay"/>
			</context>

			<context name="TypeDeclaration" attribute="NormalText" lineEndContext="#pop">
        <keyword attribute="Type" String="type" context="#stay"/>
        <keyword attribute="Type" String="type2" context="#stay"/>
        <RangeDetect attribute="TypeLabelString" char="&quot;" char1="&quot;" context="#stay"/>
        <StringDetect attribute="Keyword" String="as" context="#stay"/>
        <DetectIdentifier attribute="TypeLabel" context="#stay"/>
        <DetectChar attribute="NormalText" char="{" context="#pop" lookAhead="true"/>
			</context>

			<context name="FindLinkHead" attribute="Link" lineEndContext="#pop">
        <keyword attribute="Link" String="arrowdir" context="#stay"/>
        <RegExpr attribute="Link" String="%1*(?:&gt;\||\(\)|[&gt;o\*x\#\+\^\{])" context="#pop"/>
        <RegExpr attribute="Normal" String="[^&gt;o\*x\#\+\^\{]" context="#pop" lookAhead="true"/>
			</context>
			<context name="FindLinkTailOrOtherHead" attribute="Link" lineEndContext="#pop">
        <keyword attribute="Link" String="arrowdir" context="#stay"/>
        <RegExpr attribute="Link" String="%1*[&gt;o\*x\#\+\^\{]?" context="#stay"/>
        <RegExpr attribute="Normal" String="[^%1&gt;o\*x\#\+\^\{]*" context="#pop" lookAhead="true"/>
			</context>
    </contexts>
